<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net471</TargetFramework>
    <AssemblyName>SkbKontur.Cassandra.Primitives</AssemblyName>
    <RootNamespace>SKBKontur.Catalogue.CassandraPrimitives</RootNamespace>
    <PackageId>$(AssemblyName)</PackageId>
    <VersionPrefix>2.0.0</VersionPrefix>
    <Version>$(VersionPrefix)-rc6</Version>
    <AssemblyVersion>$(VersionPrefix).0</AssemblyVersion>
    <Authors>Ivan Burmistrov, Fedor Fominykh, Evgeny Tikhonov</Authors>
    <PackageTags>cassandra event log</PackageTags>
    <Description>.NET library implementing various primitives using Apache Cassandra</Description>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/skbkontur/cassandra-primitives</RepositoryUrl>
    <PackageProjectUrl>$(RepositoryUrl)</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" PrivateAssets="All" />
    <PackageReference Include="morelinq" Version="2.10.0" />
    <PackageReference Include="SkbKontur.Cassandra.DistributedLock" Version="2.0.0-rc6" />
  </ItemGroup>

</Project>
